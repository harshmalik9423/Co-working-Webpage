<div class="row">
    <div class="col-12">
        <h1 class="mt-3">Welcome <%= name %>, you've logged in Successfully!</h1>
    </div>
    <div class="col-12">
        <h3>Total number of logged in users are: <%= activeUsers %></h3>
    </div>
</div>

<div class="row">
    <div class="col-2" style="position: relative; left: 0; top: 0;">
        <img class="rounded-circle" style="position: relative; top: 0; left: 0;" src=<%= resultUsers[0].icon %> alt="<%= resultUsers[0].email %>_icon" width=30%/>
        <% if(activeUsers >1) { %>
            <img class="rounded-circle" style="position: absolute; top: 0; left: 40px;" src=<%= resultUsers[1].icon %> alt="<%= resultUsers[1].email %>_icon" width=30%/>
        <% } %>
        <% if(activeUsers >2) { %>
            <img class="rounded-circle" style="position: absolute; top: 0; left: 75px; z-index:1;"  src=<%= resultUsers[2].icon %> alt="<%= resultUsers[2].email %>_icon" width=30%/>
        <% } %>
        <% if(activeUsers >3) { %>
            <img class="rounded-circle" style="position: absolute; top: 0; left: 110px; z-index:2;" src=<%= resultUsers[3].icon %> alt="<%= resultUsers[3].email %>_icon" width=40%/>
        <% } %>
        <% if(activeUsers >4) { %>
            <img class="rounded-circle" style="position: absolute; top: 0; left: 145px; z-index:3;" src=<%= resultUsers[4].icon %> alt="<%= resultUsers[4].email %>_icon" width=40%/>
        <% } %>
        <% if(activeUsers >5) { %>
            <h6 style="position: absolute; top: 0; left: 180px; z-index:4;">+ <%= activeUsers-5 %></h6>
        <% } %>
    </div>
</div>
<div class="row">
    <h3>List of Users who have previously logged in:</h3>
    <ul class="list-group">
        <% Log.forEach(function(entry) { %>
            <li class="list-group-item"><%= entry.email %> &nbsp; -> logged in at : <%= entry.Log %></li>
        <% }); %>
    </ul>
</div>
<a href="/users/logout" class="btn btn-secondary">Logout</a>